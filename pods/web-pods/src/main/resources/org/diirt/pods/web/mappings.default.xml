<mappings version="1">
    <!-- By default, allow simulated data -->
    <mapping channel="sim/(.*)" substitution="sim://$1" permission="READ_ONLY"/>
    <!-- By default, allow local variables -->
    <mapping channel="loc/(.*)" substitution="loc://$1" permission="READ_WRITE"/>
