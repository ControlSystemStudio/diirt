<?xml version="1.0" encoding="UTF-8"?>
<!-- Varies the hue component of the HSV color model. 
     The map is useful for displaying periodic functions. -->
<colormap position = "relative" colorNaN="BLACK">
    <color position="0.0" value="rgb(0,0,138)"> </color>
    <color position="0.2" value="BLUE"> </color>
    <color position="0.4" value="CYAN"> </color>
    <color position="0.6" value="YELLOW"> </color>
    <color position="0.8" value="RED"> </color>
    <color position="1.0" value="rgb(138,0,0)"> </color>
</colormap>